 <h1>Welcome to SoftUni Wiki!</h1>
                <div class="spacer">
                    <h2>{{title}}
                        <div class="controls">
                            <a href="/articles/edit/{{_id}}">[edit]</a>
                            <a href="/articles/history/{{_id}}">[history]</a>
                        </div>
                    </h2>
                    <article>
                    {{#withLast edits}}                        
                        {{#each contents}}
                            <p>{{this}}</p>
                 
                        {{/each}}
                    {{/withLast}}
                    </article>
                </div>